from socket import *

messageSent = 0

def Attack():
    while True:
        mySocket = socket(AF_INET, SOCK_STREAM)
        myPort = 1024
        mySocket.connect(("131.229.158.218", myPort))
        mySocket.send(b"\Hello".encode())
        messageSent+1
        print(messageSent)
        Attack()



if __name__ == "__main__":
    Attack()


