import socket
import sys

print("try")
try:
    for port in range (1,5000):
        sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        result = sock.connect_ex(('131.229.199.189', port))
        if result == 0:
            print("Port {}: is open".format(port))
        sock.close()


except socket.error:
    print("error!")
    sys.exit()