from socket import *

print("scanning for open ports...")
try:
    for port in range (1,5000):
        mySocket = socket(AF_INET, SOCK_STREAM)
        result = mySocket.connect_ex(('131.229.199.189', port))
        if result == 0:
            print('port '+port+' is open')
        mySocket.close()


except error:
    print('error!')
    mySocket.close()